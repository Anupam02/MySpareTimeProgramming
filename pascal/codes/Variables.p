{------------------ variables illustration -------------}
program Variables(output);
{ Variable Declarations; }
var
    a, b : integer;
    x    : real;
{ Program Block }
begin
    a := 55; b := 1; x := 2.3E+03;
    writeln('Contents of a=',a);
    writeln('Contents of b=',b);
    writeln('Contents of x=',x);
end.

